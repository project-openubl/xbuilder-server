package rules.standard;

import org.openublpe.xmlbuilder.models.input.standard.DocumentInputModel;
import org.openublpe.xmlbuilder.models.output.standard.DocumentOutputModel;
import org.openublpe.xmlbuilder.models.input.common.FirmanteInputModel;
import org.openublpe.xmlbuilder.models.output.common.FirmanteOutputModel;

dialect "java"

rule "Firmante"
when
    $input : FirmanteInputModel()
    $output : FirmanteOutputModel()
then
    $output.setRuc($input.getRuc());
    $output.setRazonSocial($input.getRazonSocial());
end
